author={Ayivor, J.S. and Gordon, C. and Tobin, G.A. and Ntiamoa-Baidu, Y.},
title={Evaluation of management effectiveness of protected areas in the Volta Basin, Ghana: perspectives on the methodology for evaluation, protected area financing and community participation},
journal={Journal of Environmental Policy and Planning},
year={2020},
volume={22},
number={2},
pages={239-255},
doi={10.1080/1523908X.2019.1705153},
note={cited By 13},
