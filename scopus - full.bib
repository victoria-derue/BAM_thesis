@ARTICLE{Theodore2020,
author={Theodore, P. and Thomas, H. and Adonis, V. and Theophanis, K. and Emmanouel, O. and Charalampos, D.},
title={Erosion status of a sea cliff promontory bounding an ecologically important beach},
