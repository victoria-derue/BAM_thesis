@ARTICLE{Giglio2019,
author={Giglio, V.J. and Moura, R.L. and Gibran, F.Z. and Rossi, L.C. and Banzato, B.M. and Corsso, J.T. and Pereira-Filho, G.H. and Motta, F.S.},
title={Do managers and stakeholders have congruent perceptions on marine protected area management effectiveness?},
journal={Ocean and Coastal Management},
year={2019},
volume={179},
doi={10.1016/j.ocecoaman.2019.104865},
art_number={104865},
note={cited By 14},
