@ARTICLE{Lucieer2023,
author={Lucieer, V. and Flukes, E. and Monk, J. and Walsh, P.},
title={Geomorphometric maps of Australia’s Marine Park estate and their role in improving the integrated monitoring and management of marine ecosystems},
journal={Frontiers in Marine Science},
year={2023},
